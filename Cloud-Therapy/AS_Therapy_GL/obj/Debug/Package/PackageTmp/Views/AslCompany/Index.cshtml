@model IEnumerable<AS_Therapy_GL.Models.AslCompany>

@{
    ViewBag.Title = "Index";

    if (HttpContext.Current.Session["LoggedUserType"] == null)
    {
        Response.Redirect(Url.Action("Index", "Login"));
    }

    var LoggedUserTp = HttpContext.Current.Session["LoggedUserType"].ToString();
    var LoggedUserStatus = HttpContext.Current.Session["LoggedUserStatus"].ToString();
    if (LoggedUserTp == "AslSuperadmin" && LoggedUserStatus == "A")
    {

    }
    else
    {
        Response.Redirect(Url.Action("Index", "Logout"));
    }

}



<div id="contentHeaderBox">
    <h1>Company List</h1>
</div>
<br />

<div class="col-md-12">
    <div class="panel panel-info">
        <div class="table-responsive">
            <table class="table table-striped">
                <tr>
                    @*<th>
                            @Html.DisplayNameFor(model => model.COMPID)
                        </th>*@
                    <th>
                        @Html.DisplayNameFor(model => model.COMPID)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.COMPNM)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ADDRESS)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CONTACTNO)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EMAILID)
                    </th>
                    @*<th>
                        @Html.DisplayNameFor(model => model.WEBID)
                    </th>*@
                    @*<th>
                        @Html.DisplayNameFor(model => model.STATUS)
                    </th>*@
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        @*<td>
                                @Html.DisplayFor(modelItem => item.COMPID)
                            </td>*@
                        <td>
                            @{
                    string res = Convert.ToString(item.COMPID);
                    string res1 = Convert.ToString(item.STATUS);
                    string final = res + "_" + res1;
                    var ans = final;
                            }
                            @ans
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.COMPNM)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ADDRESS)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CONTACTNO)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EMAILID)
                        </td>
                        @*<td>
                            @Html.DisplayFor(modelItem => item.WEBID)
                        </td>*@
                        @*<td>
                            @Html.DisplayFor(modelItem => item.STATUS)
                        </td>*@
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.AslCompanyId }) |
                            @Html.ActionLink("Details", "Details", new { id = item.AslCompanyId })
                            @*@Html.ActionLink("Delete", "Delete", new { id = item.AslCompanyId })*@
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>

